{
  "name": "Cesta",
  "description": "Le panier universel qui vous permet de commander sur tous les sites en 1 seule fois!",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Cesta"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
